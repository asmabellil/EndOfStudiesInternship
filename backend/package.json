{
  "name": "net.ts",
  "version": "1.1.4",
  "description": "Node Express Template written in TypeScript NET.ts based on nodebestpractices",
  "main": "src/server.ts",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "server:prod": "node dist/server.js",
    "server:dev": "ts-node-dev --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest -c jest.config.js --env=node",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "prettier": "prettier --check \"{*.js,!(node*)**/*.ts}\"",
    "prettier:fix": "prettier --write \"{*.js,!(node*)**/*.ts}\"",
    "loglevel:change": "runtime-node-refresh"
  },
  "keywords": [
    "express template",
    "node template",
    "node express template",
    "node express template ts",
    "typescript template",
    "express",
    "boilerplate",
    "best practices node",
    "boilerplate node",
    "node ts template"
  ],
  "author": "nowicki.przemek@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "apicache": "^1.6.3",
    "bcrypt": "^5.1.1",
    "bull": "^4.13.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^14.3.2",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.8",
    "http-status": "^1.5.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "mqtt": "^5.5.0",
    "mysql2": "^3.9.2",
    "node-mocks-http": "^1.11.0",
    "nodemailer": "^6.9.14",
    "nodemailer-express-handlebars": "^6.1.2",
    "normalize-package-data": "^6.0.0",
    "oracledb": "^6.5.1",
    "path": "^0.12.7",
    "pdfkit": "^0.15.0",
    "runtime-node-refresh": "^1.0.6",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "sequelize-oracle": "^3.3.2-0.0",
    "socket.io": "^4.7.5",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.2",
    "uuid": "^9.0.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "multer": "^1.4.5-lts.1",
    "prettier": "^2.5.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0"
  }
}
